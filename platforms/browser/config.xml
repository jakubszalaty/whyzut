<?xml version='1.0' encoding='utf-8'?>
<widget id="com.szalaty.whyzut" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>whyzut</name>
    <description>
        Mobile app for edziekanat.zut.edu.pl.
    </description>
    <author email="nnaisur@gmail.com" href="https://github.com/nnaisur/">
        Kuba Szalaty
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
